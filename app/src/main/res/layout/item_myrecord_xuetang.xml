                    android:layout_weight="1"
                    android:padding="@dimen/layout_distance"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_up_result_two"
                        android:layout_width="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:id="@+id/iv_up_info_two"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:src="@mipmap/bl_icon_lan"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/tv_up_range_two"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:text="4.4-7.0"
                    android:gravity="right|center_vertical"
                    android:padding="@dimen/layout_distance"
                    android:layout_height="fill_parent" />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:background="@color/color_line"
                android:layout_height="1px" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="0dp">

                <TextView
                    android:id="@+id/tv_down_name_two"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:text="后"
                    android:gravity="center_vertical"
                    android:padding="@dimen/layout_distance"
                    android:layout_height="fill_parent" />
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:padding="@dimen/layout_distance"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/tv_down_result_two"
                        android:layout_width="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:layout_height="wrap_content" />
                    <ImageView
                        android:id="@+id/iv_down_info_two"
                        android:layout_alignParentRight="true"
                        android:layout_width="wrap_content"
                        android:src="@mipmap/bl_icon_lan"
                        android:layout_height="wrap_content" />
                </RelativeLayout>
                <TextView
                    android:id="@+id/tv_down_range_two"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:text="4.4-10.0"
                    android:gravity="right|center_vertical"
                    android:padding="@dimen/layout_distance"
                    android:layout_height="fill_parent" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:background="@color/color_line"
        android:layout_height="1px" />
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:background="@color/colorWhite"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_time_three"
